{
  "name": "node-typescript-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "someshkumarbhasskar",
  "license": "ISC",
  "scripts": {
    "build-ts": "tsc",
    "build": "npm run build-ts && npm run lint",
    "debug": "npm run build && npm run watch-debug",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve-debug": "nodemon --inspect dist/server.js",
    "serve": "node dist/server.js",
    "start": "npm run serve",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\""
  },
  "dependencies": {
    "async": "3.2.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "^2.8.5",
    "dotenv": "8.2.0",
    "errorhandler": "1.5.1",
    "express": "4.17.1",
    "express-fileupload": "^1.2.1",
    "express-validator": "6.9.2",
    "firebase-admin": "^9.11.1",
    "jsonwebtoken": "^8.5.1",
    "lusca": "1.6.1",
    "mongoose": "5.11.15",
    "nodemailer": "^6.4.17",
    "redis": "^3.1.2",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/async": "3.2.5",
    "@types/bcrypt-nodejs": "0.0.31",
    "@types/body-parser": "1.19.0",
    "@types/chai": "4.2.14",
    "@types/compression": "1.7.0",
    "@types/concurrently": "5.2.1",
    "@types/cors": "^2.8.12",
    "@types/errorhandler": "1.5.0",
    "@types/eslint": "7.2.6",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.22",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lusca": "1.6.2",
    "@types/node": "14.14.25",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.31",
    "@types/request": "2.48.5",
    "@types/request-promise": "4.1.47",
    "@types/shelljs": "0.8.8",
    "@types/supertest": "2.0.10",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "chai": "4.3.0",
    "concurrently": "6.0.2",
    "eslint": "7.19.0",
    "jest": "26.6.3",
    "nodemon": "2.0.7",
    "sass": "1.32.6",
    "shelljs": "0.8.4",
    "supertest": "6.1.3",
    "ts-jest": "26.5.0",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  }
